{
  "name": "todomvc-flux",
  "version": "0.0.1",
  "description": "Example Flux architecture.",
  "main": "js/app.js",
  "dependencies": {
    "react": "~0.10",
    "fluxy": "0.3.0-beta2"
  },
  "devDependencies": {
    "fluxy": "0.3.0-beta4",
    "browserify": "~2.36.0",
    "envify": "~1.2.0",
    "reactify": "~0.4.0",
    "statics": "~0.1.0",
    "uglify-js": "~2.4.13",
    "watchify": "~0.4.1"
  },
  "scripts": {
    "start": "STATIC_ROOT=./static watchify -o js/bundle.js -v -d .",
    "build": "STATIC_ROOT=./static NODE_ENV=production browserify . | uglifyjs -cm > js/bundle.min.js",
    "collect-static": "collect-static . ./static"
  },
  "author": "Justin Reidy",
  "license": "Apache 2",
  "browserify": {
    "transform": [
      "reactify",
      "envify"
    ]
  }
}
